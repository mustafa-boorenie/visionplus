{
  "name": "ai-playwright-scripter",
  "version": "1.0.0",
  "description": "Automated browser actions using Playwright with ChatGPT Vision analysis",
  "main": "dist/main/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "ai-playwright": "./dist/cli/intelligent-cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist screenshots/*.png reports/*.json",
    "example:basic": "tsx examples/basic-navigation.ts",
    "example:form": "tsx examples/form-filling.ts",
    "example:accessibility": "tsx examples/accessibility-check.ts",
    "example:portal": "tsx examples/portal-automation.ts",
    "example:intelligent": "tsx examples/intelligent-automation.ts",
    "example:advanced": "tsx examples/advanced-automation.ts",
    "script": "tsx scripts/run-cached-scripts.ts",
    "test": "playwright test",
    "typecheck": "tsc --noEmit",
    "postinstall": "mkdir -p screenshots reports logs scripts scripts/cache tests/generated sequences sequences/backup electron/uploads",
    "ai": "tsx src/cli/intelligent-cli.ts",
    "ai:build": "npm run build && npm link",
    "demo": "tsx demo.ts",
    "interactive": "tsx src/cli/interactive-mode.ts",
    "interactive:mcp": "USE_MCP_BROWSER=true tsx src/cli/interactive-mode.ts",
    "mcp:start": "npx playwright-mcp --headed --port 3100",
    "mcp:test": "USE_MCP_BROWSER=true tsx test-mcp-mode.ts",
    "electron:dev": "electron-vite dev",
    "electron:build": "electron-vite build",
    "electron:preview": "electron-vite preview",
    "electron:pack": "npm run electron:build && electron-builder",
    "electron:pack:mac": "npm run electron:build && electron-builder --mac",
    "electron:pack:win": "npm run electron:build && electron-builder --win",
    "electron:pack:linux": "npm run electron:build && electron-builder --linux"
  },
  "keywords": [
    "playwright",
    "automation",
    "ai",
    "vision",
    "openai",
    "web-scraping",
    "testing"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@electron-toolkit/utils": "^2.0.1",
    "@playwright/mcp": "^0.0.32",
    "@playwright/test": "^1.49.0",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.2",
    "commander": "^12.1.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.4.7",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.1.7",
    "express": "^4.18.2",
    "fs-extra": "^11.2.0",
    "js-tiktoken": "^1.0.15",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.73.0",
    "playwright": "^1.49.0",
    "socket.io": "^4.7.2",
    "uuid": "^11.1.0",
    "vectra": "^0.9.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@tanstack/react-query": "^5.12.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.10.1",
    "@types/react": "^18.2.45",
    "@types/react-beautiful-dnd": "^13.1.7",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "date-fns": "^2.30.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "electron-vite": "^2.0.0",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.294.0",
    "nodemon": "^3.1.7",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-hotkeys-hook": "^4.4.1",
    "react-json-view": "^1.21.3",
    "tailwindcss": "^3.3.6",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite": "^5.0.10"
  },
  "build": {
    "appId": "com.ai-playwright-scripter",
    "productName": "AI Playwright Scripter",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "resources/**/*"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
